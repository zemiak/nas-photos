#/bin/sh

export JAVA_HOME=${JAVA_HOME:-/Library/Java/JavaVirtualMachines/jdk1.8.0_40.jdk/Contents/Home/}
mvn -q clean || exit 1
mvn -q integration-test || exit 2
mvn -q clean || exit 3
mvn -Pprod package || exit 4
test -d ../docker/containers/nasphotos && cp target/nasphotos.war ../docker/containers/nasphotos/
echo "Done."
